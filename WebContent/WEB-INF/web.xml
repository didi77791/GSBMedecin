<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>SiteMedecins</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  	<servlet>
		<servlet-name>Accueil</servlet-name>
		<servlet-class>fr.gsbmedecin.controllers.Accueil</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Accueil</servlet-name>
		<url-pattern>/accueil</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>RechercheParPays</servlet-name>
		<servlet-class>fr.gsbmedecin.controllers.RechercheMedecinParPays</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RechercheParPays</servlet-name>
		<url-pattern>/recherche-par-pays</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>RechercheParDepartement</servlet-name>
		<servlet-class>fr.gsbmedecin.controllers.RechercheMedecinParDepartement</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RechercheParDepartement</servlet-name>
		<url-pattern>/recherche-par-departement</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>RechercheParSpecialite</servlet-name>
		<servlet-class>fr.gsbmedecin.controllers.RechercheMedecinParSpecialite</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RechercheParSpecialite</servlet-name>
		<url-pattern>/recherche-par-specialite</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>Recherche</servlet-name>
		<servlet-class>fr.gsbmedecin.controllers.Recherche</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Recherche</servlet-name>
		<url-pattern>/recherche</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>/css/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>/js/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>/images/*</url-pattern>
	</servlet-mapping>
	
</web-app>